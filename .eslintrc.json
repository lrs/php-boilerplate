{
	"extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "no-undef": 1,
    "no-unused-vars": 1,
    "indent": [1, 2, {"SwitchCase": 1}]
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "SwipeDetect": false,
    "TweenLite": false,
    "TweenMax": false,
    "Power2": false,
    "TimelineLite": false,
    "TimelineMax": false,
    "Elastic": false,
    "scrollReveal": false
  }
}
